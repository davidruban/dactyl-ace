{
    chosen {
        zmk,kscan = &kscan0;
    };

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        row-gpios = <&gpio0 29>, <&gpio0 2>, <&gpio1 15>, <&gpio1 13>, <&gpio1 11>, <&gpio0 10>;
        col-gpios = <&gpio0 20>, <&gpio0 22>, <&gpio0 24>, <&gpio1 0>, <&gpio0 11>, <&gpio1 4>;
        diode-direction = "col2row";
    };
};